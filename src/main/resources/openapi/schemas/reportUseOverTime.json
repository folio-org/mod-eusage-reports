{
  "description": "report data entry (agreement line information)",
  "type": "object",
  "properties": {
    "agreementId": {
      "type": "string",
      "description": "agreement identifier (UUID)"
    },
    "accessCountTimeSlots": {
      "description": "List of time slots, for example [\"2020-03\", \"2020-04\", \"2020-05\"]",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "journals": {
      "type": "object",
      "description": "Journals report",
      "properties": {
        "totalItemRequests": {
          "type": "array",
          "description": "access count per time slot, sum of all journals",
          "items": {
            "type": "integer"
          }
        },
        "uniqueItemRequests": {
          "type": "array",
          "description": "access count per time slot, sum of all journals",
          "items": {
            "type": "integer"
          }
        },
        "journals": {
          "description": "List of journals",
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "reportUseOverTimeJournal.json"
          }
        },
        "required": [
          "totalItemRequests", "uniqueItemRequests", "journals"
        ]
      }
    }
  },
  "required": [
    "agreementId", "accessCountTimeSlots"
  ]
}
